             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <rib:RibbonGroup Header="WinRM" Name="_rgExplorer">
            <rib:RibbonButton Label="enable WinRM" Name="btEnableWinRM" SmallImageSource="Images/Server upgrade.ico" LargeImageSource="Images/Server upgrade.ico" ToolTip="enable PowerShell-Remoting (WinRM)" Click="btEnableWinRM_Click" >
            </rib:RibbonButton>
        </rib:RibbonGroup>
