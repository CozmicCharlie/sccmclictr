            <setting name="Password" serializeAs="String">
                <value />
            </setting>
            <setting name="DefaultHostName" serializeAs="String">
                <value>127.0.0.1</value>
            </setting>
        </ClientCenter.Properties.Settings>
    </userSettings>
</configuration>
